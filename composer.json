    "require": {
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "hiqdev/yii2-thememanager": "<2.0 || dev-master"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0",
        "hiqdev/hidev-hiqdev": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hiqdev\\themes\\flat\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "hisite": "src/config/hisite.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
